@model Vidly.ViewModels.ListCustomerViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<ul>
    @foreach (var customer in Model.Customers)
    {
        
        <li> <a href="Customers/@customer.Id"> <p>@customer.Name</p> <p>@customer.MembershipType.Name</p></a> </li>
    }
</ul>